{
	"name": "mini-starterkit",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build:dev": "cross-env parcel src/html/*.html --open",
		"build:prod": "cross-env NODE_ENV=production parcel build src/html/*.html --out-dir public --public-url ./ --no-cache --no-source-maps --no-minify"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/polyfill": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"autoprefixer": "^9.1.3",
		"cross-env": "^5.2.0",
		"node-sass": "^4.9.3",
		"normalize.css": "^8.0.1",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-clean-easy": "^1.0.2",
		"postcss-modules": "^1.3.2"
	},
	"parcelCleanPaths": [
		"dist",
		"public"
	],
	"dependencies": {}
}
